{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "cwd": "../Bank"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml", "**/*.md" ],
        "src": "api"
      },
      {
        "files": [ "**/*.md" ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Bank API Documentation"
    }
  }
}
